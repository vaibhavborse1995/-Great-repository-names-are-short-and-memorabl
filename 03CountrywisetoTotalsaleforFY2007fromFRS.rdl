  <DataSets>
    <DataSet Name="Countriwisetotalsaleforfy2007fromFRS">
      <Query>
        <DataSourceName>DWDW2012</DataSourceName>
        <CommandText>SELECT DG.EnglishCountryRegionName AS COUNTRY, SUM(FRS.SalesAmount) AS TotalSale
FROM   FactResellerSales AS FRS INNER JOIN
             DimDate AS DD ON FRS.OrderDateKey = DD.DateKey INNER JOIN
             DimReseller AS DR ON FRS.ResellerKey = DR.ResellerKey INNER JOIN
             DimGeography AS DG ON DR.GeographyKey = DG.GeographyKey
WHERE (DD.CalendarYear = 2007)
GROUP BY DG.EnglishCountryRegionName</CommandText>
      </Query>
      <Fields>
        <Field Name="COUNTRY">
          <DataField>COUNTRY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalSale">
          <DataField>TotalSale</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
